{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-minimal-notebook-gpu",
                "labels": {
                    "build": "s2i-minimal-notebook-gpu"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-minimal-notebook-gpu",
                "labels": {
                    "build": "s2i-minimal-notebook-gpu"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/vpavlin/jupyter-notebooks"
                    },
                    "contextDir": "minimal-notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "DockerImage",
                            "name": "quay.io/vpavlin/python-36-centos7:cuda"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-minimal-notebook-gpu:3.6"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "1Gi"
                    }
                }
            }
        },
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-tensorflow-notebook-gpu",
                "labels": {
                    "build": "s2i-tensorflow-notebook-gpu"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "s2i-tensorflow-notebook-gpu",
                "labels": {
                    "build": "s2i-tensorflow-notebook-gpu"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/vpavlin/jupyter-notebooks",
                        "ref": "feature/add-tf-gpu"
                    },
                    "contextDir": "tensorflow-notebook-gpu"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "s2i-minimal-notebook-gpu:3.6"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-tensorflow-notebook-gpu:3.6"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "512Mi"
                    }
                }
            }
        }
    ]
}
